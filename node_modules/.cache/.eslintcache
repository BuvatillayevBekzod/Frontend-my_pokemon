h} /> {/* Include the Navbar component */}\n            <div className=\"container\">\n                <div className=\"left-content scroll\">\n                    <Card\n                        pokemon={searching ? filteredPokemon : pokeData}\n                        loading={loading}\n                        infoPokemon={(poke) => setPokeDex(poke)}\n                    />\n                    <div className=\"btn-group\">\n                        {prevUrl && (\n                            <button\n                                onClick={() => {\n                                    setPokeData([]);\n                                    setUrl(prevUrl);\n                                }}\n                            >\n                                Previous\n                            </button>\n                        )}\n                        {nextUrl && (\n                            <button\n                                onClick={() => {\n                                    setPokeData([]);\n                                    setUrl(nextUrl);\n                                }}\n                            >\n                                Next\n                            </button>\n                        )}\n                    </div>\n                </div>\n                <div className=\"right-content\">\n                    <Pokeinfo data={pokeDex} />\n                </div>\n            </div>\n        </>\n    );\n};\n\nexport default Main;\n","/Users/student/Desktop/Bekzod/Frontend-my_pokemon/src/Components/Pokeinfo.jsx",[],[],"/Users/student/Desktop/Bekzod/Frontend-my_pokemon/src/Components/Navbar.jsx",["35"],[],"/Users/student/Desktop/Bekzod/Frontend-my_pokemon/src/Components/Card.jsx",[],[],["36","37"],{"ruleId":"38","severity":1,"message":"39","line":39,"column":8,"nodeType":"40","endLine":39,"endColumn":13,"suggestions":"41"},{"ruleId":"42","severity":1,"message":"43","line":2,"column":10,"nodeType":"44","messageId":"45","endLine":2,"en